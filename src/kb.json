        "Requires flexible or surface-based fastening"
      ]
    }
  ],
  "rules": [
    {
      "id": "paper_to_paper",
      "context": "Handles very weak paper joints by excluding unsuitable fastening categories.",
      "conditions": {
        "materials.same_material": true,
        "materials.material_a.material_type": "paper"
      },
      "effects": {
        "requirements.excluded_categories": [
          "mechanical",
          "thermal"
        ],
        "requirements.min_tensile_strength": "very_low"
      }
    },
    {
      "id": "brittle_materials_present",
      "context": "Avoids mechanical fastening when one or more materials are brittle.",
      "conditions": {
        "materials.any.brittleness": "very_high"
      },
      "effects": {
        "requirements.excluded_categories": [
          "mechanical"
        ]
      }
    },
    {
      "id": "low_porosity_materials",
      "context": "Limits adhesive use when materials have low surface porosity.",
      "conditions": {
        "materials.any.porosity": "low"
      },
      "effects": {
        "requirements.excluded_categories": [
          "adhesive"
        ]
      }
    },
    {
      "id": "metal_to_metal",
      "context": "Allows high-strength joining methods for identical metal parts.",
      "conditions": {
        "materials.same_material": true,
        "materials.material_a.material_type": "metal"
      },
      "effects": {
        "requirements.allowed_categories": [
          "mechanical",
          "thermal"
        ],
        "requirements.min_tensile_strength": "high"
      }
    },
    {
      "id": "wood_to_wood",
      "context": "Excludes thermal methods for joints between wooden materials.",
      "conditions": {
        "materials.same_material": true,
        "materials.material_a.material_type": "wood"
      },
      "effects": {
        "requirements.excluded_categories": [
          "thermal"
        ]
      }
    },
    {
      "id": "fabric_involved",
      "context": "Requires flexible bonding when fabric is part of the joint.",
      "conditions": {
        "materials.any.material_type": "fabric"
      },
      "effects": {
        "requirements.allowed_rigidities": [
          "flexible",
          "semi_flexible"
        ],
        "requirements.excluded_categories": [
          "thermal"
        ]
      }
    },
    {
      "id": "masonry_involved",
      "context": "Ensures sufficient shear strength for masonry connections.",
      "conditions": {
        "materials.any.material_type": "masonry"
      },
      "effects": {
        "requirements.min_shear_strength": "moderate"
      }
    },
    {
      "id": "splash_environment",
      "context": "Requires basic water resistance for joints exposed to splashing moisture.",
      "conditions": {
        "environment.moisture": "splash"
      },
      "effects": {
        "requirements.min_water_resistance": "fair"
      }
    },
    {
      "id": "outdoor_environment",
      "context": "Requires good water resistance for outdoor installations.",
      "conditions": {
        "environment.moisture": "outdoor"
      },
      "effects": {
        "requirements.min_water_resistance": "good"
      }
    },
    {
      "id": "submerged_environment",
      "context": "Requires excellent water resistance for fully submerged joints.",
      "conditions": {
        "environment.moisture": "submerged"
      },
      "effects": {
        "requirements.min_water_resistance": "excellent"
      }
    },
    {
      "id": "chemical_exposure",
      "context": "Ensures chemical resistance when the joint is exposed to chemicals.",
      "conditions": {
        "environment.chemical_exposure": true
      },
      "effects": {
        "requirements.min_chemical_resistance": "fair"
      }
    },
    {
      "id": "chemical_exposure_and_permanent",
      "context": "Strengthens chemical resistance requirements for permanent joints.",
      "conditions": {
        "environment.chemical_exposure": true,
        "constraints.permanence": "permanent"
      },
      "effects": {
        "requirements.min_chemical_resistance": "good"
      }
    },
    {
      "id": "static_load",
      "context": "Sets low strength requirements for purely static loads.",
      "conditions": {
        "load.load_type": "static"
      },
      "effects": {
        "load.required_strength": "low"
      }
    },
    {
      "id": "light_dynamic_load",
      "context": "Sets moderate strength requirements for lightly dynamic loads.",
      "conditions": {
        "load.load_type": "light_dynamic"
      },
      "effects": {
        "load.required_strength": "moderate"
      }
    },
    {
      "id": "heavy_dynamic_load",
      "context": "Requires high strength and vibration resistance for heavy dynamic loads.",
      "conditions": {
        "load.load_type": "heavy_dynamic"
      },
      "effects": {
        "load.required_strength": "high",
        "requirements.min_vibration_resistance": "fair"
      }
    },
    {
      "id": "vibration_present",
      "context": "Excludes weak fastening methods when vibration is present.",
      "conditions": {
        "load.vibration": true
      },
      "effects": {
        "requirements.min_vibration_resistance": "good",
        "requirements.excluded_categories": [
          "adhesive"
        ]
      }
    },
    {
      "id": "removable_connection",
      "context": "Restricts fastening methods to those suitable for removable joints.",
      "conditions": {
        "constraints.permanence": "removable"
      },
      "effects": {
        "requirements.excluded_categories": [
          "thermal"
        ],
        "requirements.allowed_permanence": [
          "removable",
          "semi_permanent"
        ]
      }
    },
    {
      "id": "permanent_connection",
      "context": "Restricts fastening methods to those suitable for permanent joints.",
      "conditions": {
        "constraints.permanence": "permanent"
      },
      "effects": {
        "requirements.allowed_permanence": [
          "permanent"
        ]
      }
    },
    {
      "id": "flexibility_required",
      "context": "Limits fastening options to flexible or semi-flexible joints.",
      "conditions": {
        "constraints.flexibility_required": true
      },
      "effects": {
        "requirements.allowed_rigidities": [
          "flexible",
          "semi_flexible"
        ]
      }
    },
    {
      "id": "vertical_or_overhead",
      "context": "Excludes adhesives for joints installed vertically or overhead.",
      "conditions": {
        "constraints.orientation_vertical": true
      },
      "effects": {
        "requirements.excluded_categories": [
          "adhesive"
        ]
      }
    },
    {
      "id": "health_constraints",
      "context": "Avoids fastening methods that pose health or safety risks.",
      "conditions": {
        "constraints.health_constraints": true
      },
      "effects": {
        "requirements.excluded_categories": [
          "thermal"
        ]
      }
    },
    {
      "id": "immediate_use_required",
      "context": "Excludes slow-curing methods when immediate usability is required.",
      "conditions": {
        "constraints.max_curing_time": "immediate"
      },
      "effects": {
        "requirements.excluded_categories": [
          "adhesive"
        ]
      }
    },
    {
      "id": "uv_exposure_present",
      "context": "Requires UV resistance when the joint is exposed to sunlight.",
      "conditions": {
        "environment.uv_exposure": true
      },
      "effects": {
        "requirements.min_uv_resistance": "good"
      }
    },
    {
      "id": "outdoor_and_uv_exposure",
      "context": "Strengthens UV resistance requirements for outdoor exposure.",
      "conditions": {
        "environment.moisture": "outdoor",
        "environment.uv_exposure": true
      },
      "effects": {
        "requirements.min_uv_resistance": "excellent"
      }
    },
    {
      "id": "no_uv_but_outdoor",
      "context": "Not specifically exposed to sunlight but located outdoors.",
      "conditions": {
        "environment.uv_exposure": false,
        "environment.moisture": "outdoor"
      },
      "effects": {
        "requirements.min_uv_resistance": "poor"
      }
    },
    {
      "id": "temperature_extremes_present",
      "context": "Require good temperature resistance when temperature extremes are present.",
      "conditions": {
        "environment.temperature_extremes": true
      },
      "effects": {
        "requirements.min_temperature_resistance": "good"
      }
    },
    {
      "id": "temperature_extremes_and_outdoor",
      "context": "Strengthens temperature resistance requirements for outdoor extremes.",
      "conditions": {
        "environment.temperature_extremes": true,
        "environment.moisture": "outdoor"
      },
      "effects": {
        "requirements.min_temperature_resistance": "excellent"
      }
    },
    {
      "id": "tension_dominant",
      "context": "Requires sufficient tensile strength for tension dominated loads.",
      "conditions": {
        "load.tension_dominant": true
      },
      "effects": {
        "requirements.min_tensile_strength": "moderate"
      }
    },
    {
      "id": "shock_loads_present",
      "context": "Requires shear strength and excludes weak adhesives under shock loads.",
      "conditions": {
        "load.shock_loads": true
      },
      "effects": {
        "requirements.min_shear_strength": "moderate",
        "requirements.excluded_categories": [
          "adhesive"
        ]
      }
    },
    {
      "id": "high_tensile_shock_implies_high_shear",
      "context": "Infers high shear strength when both shock loads and high tension occur.",
      "conditions": {
        "load.shock_loads": true,
        "requirements.min_tensile_strength": "high"
      },
      "effects": {
        "requirements.min_shear_strength": "high"
      }
    },
    {
      "id": "precision_positioning_required",
      "context": "Requires vibration resistance when precise positioning is necessary.",
      "conditions": {
        "constraints.precision_required": true
      },
      "effects": {
        "requirements.min_vibration_resistance": "good"
      }
    },
    {
      "id": "vibration_and_permanent",
      "context": "Excludes adhesives when vibration occurs in permanent joints.",
      "conditions": {
        "load.vibration": true,
        "constraints.permanence": "permanent"
      },
      "effects": {
        "requirements.excluded_categories": [
          "adhesive"
        ]
      }
    },
    {
      "id": "flexible_implies_low_strength",
      "context": "if flexibility is required then strength is most likely low",
      "conditions": {
        "constraints.flexibility_required": true
      },
      "effects": {
        "requirements.allowed_rigidities": [
          "flexible",
          "semi_flexible"
        ],
        "requirements.min_tensile_strength": "low"
      }
    },
    {
      "id": "overhead_installation_requires_shear",
      "context": "Requires high shear strength for overhead installations.",
      "conditions": {
        "constraints.orientation_vertical": true
      },
      "effects": {
        "requirements.min_shear_strength": "high"
      }
    },
    {
      "id": "high_shear_excludes_weak_adhesives",
      "context": "Excludes adhesives that cannot meet high shear strength requirements.",
      "conditions": {
        "requirements.min_shear_strength": "high"
      },
      "effects": {
        "requirements.excluded_categories": [
          "adhesive"
        ]
      }
    },
    {
      "id": "adhesive_only_requires_water_resistance",
      "context": "Require water resistance when only adhesive fastening is allowed.",
      "conditions": {
        "requirements.allowed_categories": "adhesive"
      },
      "effects": {
        "requirements.min_water_resistance": "fair"
      }
    },
    {
      "id": "high_porosity_excludes_low_strength_adhesives",
      "context": "Require strong fastening when materials have high porosity.",
      "conditions": {
        "materials.any.porosity": "high"
      },
      "effects": {
        "requirements.min_shear_strength": "moderate"
      }
    }
  ],
