{
  "questions": [
    {
      "id": "material_a_type",
      "text": "What is the first material?",
      "attribute": "materials.material_a.material_type",
      "type": "enum",
      "options": [
        "wood",
        "metal",
        "paper",
        "plastic",
        "glass",
        "ceramic",
        "stone",
        "masonry",
        "fabric"
      ],
      "helps_rules": [
        "paper_to_paper",
        "brittle_materials_present",
        "fabric_involved",
        "metal_to_metal",
        "wood_to_wood",
        "masonry_involved"
      ]
    },
    {
      "id": "material_b_type",
      "text": "What is the second material?",
      "attribute": "materials.material_b.material_type",
      "type": "enum",
      "options": [
        "wood",
        "metal",
        "paper",
        "plastic",
        "glass",
        "ceramic",
        "stone",
        "masonry",
        "fabric"
      ],
      "helps_rules": [
        "paper_to_paper",
        "brittle_materials_present",
        "fabric_involved",
        "metal_to_metal",
        "wood_to_wood",
        "masonry_involved"
      ]
    },
    {
      "id": "environment_moisture",
      "text": "What is the moisture exposure?",
      "attribute": "environment.moisture",
      "type": "enum",
      "options": [
        "none",
        "splash",
        "outdoor",
        "submerged"
      ],
      "helps_rules": [
        "splash_environment",
        "outdoor_environment",
        "submerged_environment",
        "outdoor_and_uv_exposure",
        "no_uv_but_outdoor",
        "temperature_extremes_and_outdoor"
      ]
    },
    {
      "id": "load_type",
      "text": "What type of load is applied?",
      "attribute": "load.load_type",
      "type": "enum",
      "options": [
        "static",
        "light_dynamic",
        "heavy_dynamic"
      ],
      "helps_rules": [
        "static_load",
        "light_dynamic_load",
        "heavy_dynamic_load"
      ]
    },
    {
      "id": "vibration",
      "text": "Is there vibration?",
      "attribute": "load.vibration",
      "type": "boolean",
      "ask_if": {
        "load.load_type": [
          "light_dynamic",
          "heavy_dynamic"
        ]
      },
      "helps_rules": [
        "vibration_present",
        "vibration_and_permanent"
      ]
    },
    {
      "id": "permanence",
      "text": "How permanent should the connection be?",
      "attribute": "constraints.permanence",
      "type": "enum",
      "options": [
        "removable",
        "semi_permanent",
        "permanent"
      ],
      "helps_rules": [
        "removable_connection",
        "permanent_connection",
        "chemical_exposure_and_permanent",
        "vibration_and_permanent"
      ]
    },
    {
      "id": "uv_exposure",
      "text": "Will the connection be exposed to sunlight (UV)?",
      "attribute": "environment.uv_exposure",
      "type": "boolean",
      "ask_if": {
        "environment.moisture": [
          "outdoor",
          "submerged"
        ]
      },
      "helps_rules": [
        "uv_exposure_present",
        "outdoor_and_uv_exposure",
        "no_uv_but_outdoor"
      ]
    },
    {
      "id": "temperature_extremes",
      "text": "Will the connection experience high or low temperature extremes?",
      "attribute": "environment.temperature_extremes",
      "type": "boolean",
      "ask_if": {
        "environment.moisture": [
          "outdoor",
          "submerged"
        ]
      },
      "helps_rules": [
        "temperature_extremes_present",
        "temperature_extremes_and_outdoor"
      ]
    },
    {
      "id": "load_direction",
      "text": "Is the load primarily pulling (tension) rather than pressing (compression)?",
      "attribute": "load.tension_dominant",
      "type": "boolean",
      "ask_if": {
        "load.load_type": [
          "light_dynamic",
          "heavy_dynamic"
        ]
      },
      "helps_rules": [
        "tension_dominant"
      ]
    },
    {
      "id": "shock_loads",
      "text": "Will the connection experience sudden impacts or shocks?",
      "attribute": "load.shock_loads",
      "type": "boolean",
      "ask_if": {
        "load.load_type": [
          "light_dynamic",
          "heavy_dynamic"
        ]
      },
      "helps_rules": [
        "shock_loads_present",
        "high_tensile_shock_implies_high_shear"
      ]
    },
    {
      "id": "access_one_side",
      "text": "Is only one side of the materials accessible during installation?",
      "attribute": "constraints.one_sided_access",
      "type": "boolean",
      "helps_rules": []
    },
    {
      "id": "precision_required",
      "text": "Is precise positioning of the materials required?",
      "attribute": "constraints.precision_required",
      "type": "boolean",
      "helps_rules": [
        "precision_positioning_required"
      ]
    },
    {
      "id": "chemical_exposure",
      "text": "Will the joint be exposed to chemicals (solvents, oils, acids)?",
      "attribute": "environment.chemical_exposure",
      "type": "boolean",
      "helps_rules": [
        "chemical_exposure",
        "chemical_exposure_and_permanent"
      ]
    },
    {
      "id": "flexibility_required",
      "text": "Does the joint need to remain flexible after installation?",
      "attribute": "constraints.flexibility_required",
      "type": "boolean",
      "helps_rules": [
        "flexibility_required",
        "fabric_involved",
        "flexible_implies_low_strength"
      ]
    },
    {
      "id": "orientation_vertical",
      "text": "Is the joint installed vertically or overhead?",
      "attribute": "constraints.orientation_vertical",
      "type": "boolean",
      "helps_rules": [
        "vertical_or_overhead",
        "overhead_installation_requires_shear"
      ]
    },
    {
      "id": "health_constraints",
      "text": "Are there health or safety constraints (fumes, heat, toxicity)?",
      "attribute": "constraints.health_constraints",
      "type": "boolean",
      "helps_rules": [
        "health_constraints"
      ]
    },
    {
      "id": "max_curing_time",
      "text": "Must the joint be usable immediately after installation?",
      "attribute": "constraints.max_curing_time",
      "type": "enum",
      "options": [
        "immediate",
        "fast",
        "moderate",
        "slow"
      ],
      "helps_rules": [
        "immediate_use_required"
      ]
    }
  ],
  "materials": [
